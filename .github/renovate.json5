{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  assignees: [
    'nzbr',
  ],
  reviewers: [
    'nzbr',
  ],
  automergeStrategy: 'squash',
  labels: [
    'renovate',
  ],
  nix: {
    enabled: true,
  },
  lockFileMaintenance: {
    enabled: true,
    extends: [
      'schedule:weekly',
    ],
    automerge: true,
  },
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/^.*$/',
      ],
      matchStrings: [
        'https://github.com/(?<depName>[^/]+/[^/]+)/releases/download/(?<currentValue>[^/\\s"]+)',
      ],
      datasourceTemplate: 'github-releases',
    },
    {
      customType: 'regex',
      managerFilePatterns: [
        '/^.*.ts$/',
      ],
      matchStrings: [
        'fetchGit\\s*\\(\\s*"(?<depName>[^"]+)"\\s*,\\s*"(?<currentValue>[^"]+)',
      ],
      datasourceTemplate: 'git-tags',
    },
    {
      customType: 'regex',
      managerFilePatterns: [
        '/^.*.nix$/',
      ],
      matchStrings: [
        '"github:(?<depName>[^/]+/[^/]+)/(?<currentValue>[^/"]+)"',
      ],
      datasourceTemplate: 'github-tags',
    },
  ],
}
